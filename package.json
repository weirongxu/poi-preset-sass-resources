{
  "name": "poi-preset-sass-resources",
  "version": "4.0.0",
  "description": "Add sass-resources support in Poi.",
  "main": "index.js",
  "repository": "https://github.com/weirongxu/poi-preset-sass-resources.git",
  "author": "Weirong Xu <weirongxu.raidou@gmail.com>",
  "homepage": "https://github.com/weirongxu/poi-preset-sass-resources",
  "license": "MIT",
  "keywords": [
    "plugin",
    "poi",
    "poi-plugin",
    "poi-preset",
    "sass-resources",
    "sass-resources-loader"
  ],
  "files": [
    "index.js"
  ],
  "xo": {
    "space": 2,
    "semicolon": false,
    "extends": [
      "plugin:jest/recommended"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "quote-props": "off",
      "semi-style": [
        "error",
        "first"
      ],
      "no-warning-comments": "off",
      "capitalized-comments": "off",
      "indent": [
        "error",
        2,
        {
          "MemberExpression": "off"
        }
      ]
    }
  },
  "scripts": {
    "docs": "documentation readme index.js -s API",
    "test": "jest && xo",
    "dev": "jest --watch"
  },
  "devDependencies": {
    "documentation": "^8.0.0",
    "eslint-plugin-jest": "^21.17.0",
    "glob": "^7.1.2",
    "jest": "^23.4.0",
    "node-sass": "^4.9.2",
    "pify": "^3.0.0",
    "poi": "^10.2.9",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "xo": "^0.21.1"
  },
  "dependencies": {
    "sass-resources-loader": "^1.3.3"
  }
}
