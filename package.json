{
  "name": "poi-preset-sass-resources",
  "version": "1.0.2",
  "description": "Add sass-resources support in Poi.",
  "main": "index.js",
  "repository": "https://github.com/weirongxu/poi-preset-sass-resources.git",
  "author": "Weirong Xu <weirongxu.raidou@gmail.com>",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "indent": [
        "error",
        2,
        {
          "MemberExpression": "off"
        }
      ]
    }
  },
  "scripts": {
    "docs": "documentation readme index.js -s API",
    "test": "xo && ava"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^6.2.0",
    "glob": "^7.1.2",
    "node-sass": "^4.8.3",
    "poi": "^10.1.5",
    "sass-loader": "^7.0.0",
    "xo": "^0.20.3"
  },
  "dependencies": {
    "sass-resources-loader": "^1.3.3"
  }
}
